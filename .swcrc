{
  "$schema": "https://json.schemastore.org/swcrc",

  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": false,
      "decorators": false,
      "dts": true
    },
    "target": "es5",
    "loose": false,
    "externalHelpers": false,
    "minify": {
      "compress": true,
      "mangle": true
    }
  },
  "isModule": true,
  "sourceMaps": true
}
